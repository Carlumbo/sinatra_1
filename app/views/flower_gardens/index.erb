
<% @gardens.each do |garden| %>
  <%= garden.name %> <br />
  <%= garden.size %> <br />

  <form action="/gardens/<%=garden.id%>/edit" method="POST">
  <input type="submit" value="Edit">
  </form>

  <form action="/gardens/<%=garden.id%>/delete" method="POST">
  <input type="submit" value="Destroy">
  </form>


  <br />
  <br />
<% end %>
